url http://sourceforge.net/projects/libusb/files/libusb-1.0/
SF_version_dir
version_link '[.]tar[.][a-z0-9]+/download$'
SF_redirect
do_overwrite () {
  ensure_hash
  set_var_value url "$CURRENT_URL"
  set_var_value sha256 "$CURRENT_HASH"
  set_var_value version "$CURRENT_VERSION"
  set_var_value name "$CURRENT_NAME-$CURRENT_VERSION"
}

